// Place your settings in this file to overwrite default and user settings.
{
  "editor.tabSize": 2,
  "editor.rulers": [100],
  "editor.wordWrapColumn": 100,
  "editor.formatOnSave": false,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/node_modules/": true,
    "**/dist/": true,
    "coverage": true,
    "coverage.lcov": true
  }
}
